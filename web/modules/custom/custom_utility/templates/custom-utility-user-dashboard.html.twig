<div class="dashboard">
	<h2>{{ 'User Dashboard'|t }}</h2>
	<div class="dashboard-content">
		{% if content.message %}
			<div class="message">{{ content.message }}</div>
		{% endif %}
		{% if content %}
			<div class="user-specific-content">
				{{ content|without('message') }}
			</div>
		{% else %}
			<p>No data available for this user role.</p>
		{% endif %}
	</div>
</div>
